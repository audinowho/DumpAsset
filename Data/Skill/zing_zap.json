"Name": {
"DefaultText": "Zing Zap",
"LocalTexts": {
"fr": "Électrikipik",
"de": "Elektropikser",
"es": "Electropunzada",
"it": "Elettropizzico",
"ko": "찌리리따끔따끔",
"ja": "びりびりちくちく",
