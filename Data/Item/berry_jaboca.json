"Name": {
"DefaultText": "Jaboca Berry",
"LocalTexts": {
"fr": "Baie Jaboca",
"de": "Jabocabeere",
"es": "Baya Jaboca",
"it": "Baccajaba",
"ko": "자보열매",
"ja": "ジャポのみ",
"ja-jp": "ジャポのみ",
"zh-hans": "嘉珍果",
"zh-hant": "嘉珍果"
