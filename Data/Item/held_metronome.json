"Name": {
"DefaultText": "Metronome",
"LocalTexts": {
"fr": "Métronome",
"de": "Metronom",
"es": "Metrónomo",
"it": "Plessimetro",
"ko": "메트로놈",
"ja": "メトロノーム",
